{
  "name": "zombee",
  "version": "0.5.0",
  "description": "üêù Zombee will keep interval fetch target uri until it die",
  "main": "index.js",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  },
  "keywords": [
    "zombee",
    "fetch",
    "interval",
    "cron",
    "job"
  ],
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.9.1",
    "eslint-plugin-babel": "^3.3.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.3"
  },
  "env": {
    "mocha": true,
    "node": true
  },
  "scripts": {
    "dev": "babel-node index.js",
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production && rimraf dist && webpack --config ./webpack.config.prod.js",
    "lint": "eslint src test",
    "test": "npm run lint && npm run test:once",
    "test:once": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive",
    "test:watch": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --watch",
    "prepublish": "npm test && npm run clean && npm run build"
